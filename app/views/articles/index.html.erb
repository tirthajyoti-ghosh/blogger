<h1>All Articles</h1>

<ul id="articles">
  <% @articles.each do |article| %>
    <li>
      <%= link_to article.title, article_path(article.id) %>
    </li>
  <% end %>
</ul>

<br>

<%= link_to 'Show all tags', tags_path %>

<%= link_to 'Create a new article', new_article_path, class: 'new_article' %>
